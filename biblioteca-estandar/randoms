import random

#genera rnumero entero aleatorio

random_number = random.randint(1, 10)
print(random_number)

#elegir colores aleatorios
colors = ['rojo',  'Azul',  'verde']
random_color = random.choice(colors)
print(random_color)


#Barajar lista de cartas

card = [ 'As', 'Rey',  'Reina',  'Jota']
random.shuffle(card)
print(card)